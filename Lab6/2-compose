const inc = (x) => ++x;
const twice = (x) => x * 2;
const cube = (x) => x ** 3;

function compose(...functions) {
	let calc = (x) => {
		try {
			for (let f of functions.reverse()) {
				x = f(x);
			}
			return x;
		} catch (error) {
			return undefined;
		}
	};
	return calc;
}

const f1 = compose(inc, twice, cube);
console.log(f1(5));

const f2 = compose(inc, inc);
console.log(f2(7));

const f3 = compose(inc, 7, cube);
console.log(f3(6));